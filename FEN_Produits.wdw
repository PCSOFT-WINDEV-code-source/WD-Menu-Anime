     name : InitialiseMenu
     internal_properties : CAAAAAgAAAAmUcIUFSIzAdiYByFm1DvOSTZO6qZhXQbaLBSRwSq6O4xJ68qpNGAZEmpGWM5o6d7aotbjJtg1UxKenTasboShufqonAioLx4wSKLbo2iH7J0S8tS06XYFgyDLK+YLa9JCgZJy2zzAa7869Xaff0Jv+E5U1fOkb5RflCPfhJRUiWm741FiXYTxnwyuFitvKoOx0GsCHbY=
     procedure_id : 1800465211278912172
     type_code : 14
     code : |1-
      // Résumé : Initialisation du menu
      // Syntaxe :
      //InitialiseMenu ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      
      Procédure InitialiseMenu()
      
      polPoliceOption est une police // Police des options
      polPoliceOptionSelect est une Police // Police de l'option sélectionnée
      
      
      // Ajoute les options de menu : une option par catégorie
      pour tout categorie sur nom
      	CMOD_Categorie.AjouteOption(categorie.nom, categorie.IDCatégorie, categorie.Image)
      FIN
      
      // Définit la police des options
      polPoliceOption..Couleur = iBlanc
      polPoliceOption..Taille = 10
      polPoliceOption..nom = "Open Sans"
      CMOD_Categorie.SetPoliceBouton(polPoliceOption)
      // Définit la police de l'option sélectionnée
      polPoliceOptionSelect..Couleur = iBlanc
      polPoliceOptionSelect..Taille = 10
      polPoliceOptionSelect..Nom = "Open Sans"
      polPoliceOptionSelect..Gras = vrai
      CMOD_Categorie.SetPoliceBoutonSélection(polPoliceOptionSelect)
      
      // Définit les images de fond de bouton
      CMOD_Categorie.SetImageFondBouton("FondBouton.png")
      CMOD_Categorie.SetImageFondBoutonSélection("FondSelection.png")
      // Définit la procédure CallBack à exécuter lors de la sélection d'une option
      CMOD_Categorie.SetCallbackSelection("FEN_Produits.AfficheProduit")
      
      // Sans mode un clic
      CMOD_Categorie.SetModeUnClic(faux)
      
      // Dessine le menu
      CMOD_Categorie.DessineMenu()
      
      // Exécute le code de sélection pour la 1ère option (sélectionnée par défaut)
      CMOD_Categorie.ExecuteCallBackSelection()
     type : 458752
