     code : |1-
      // Résumé : Ouvre l'option demandée
      // Syntaxe :
      //AfficheProduit (<nIdCategorie> est entier)
      //
      // Paramètres :
      //	nIdCategorie (entier) : Identifiant de l'option sur laquelle le clic a eu lieu
      // Valeur de retour :
      // 	Aucune
      //
      
      Procédure AfficheProduit(LOCAL nIdCategorie est un entier)
      
      
      // Définit la catégorie de la requête
      REQ_Produits.pIdCategorie = nIdCategorie
      
      // Réaffiche la table des produits pour cette catégorie
      tableaffiche(TABLE_Produit, taRéExecuteRequete)
