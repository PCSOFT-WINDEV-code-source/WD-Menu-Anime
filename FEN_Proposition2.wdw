   -
     name : InitialiseMenu
     procedure_id : 1259803556880415875
     type_code : 14
     code : |1-
      // Résumé : Initialisation du menu
      // Syntaxe :
      //InitialiseMenu ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      
      Procédure InitialiseMenu()
      
      polPoliceOption est une police // Police des options
      polPoliceOptionSelect est une Police // Police de l'option sélectionnée
      
      
      // Ajoute les options de menu
      CMOD_MENU.AjouteOption("Assistance", cstMENU_ASSISTANCE, cstMENU_ASSISTANCE_IMAGES)
      CMOD_MENU.AjouteOption("Clients", cstMENU_CLIENTS, cstMENU_CLIENTS_IMAGES)
      CMOD_MENU.AjouteOption("Commandes", cstMENU_COMMANDES, cstMENU_COMMANDES_IMAGES)
      CMOD_MENU.AjouteOption("Contrats", cstMENU_CONTRATS, cstMENU_CONTRATS_IMAGES)
      CMOD_MENU.AjouteOption("Factures", cstMENU_FACTURE, cstMENU_FACTURE_IMAGES)
      CMOD_MENU.AjouteOption("Produits", cstMENU_PRODUITS, cstMENU_PRODUITS_IMAGES)
      CMOD_MENU.AjouteOption("Collaborateurs", cstMENU_RESSHUMAINES, cstMENU_RESSHUMAINES_IMAGES)
      CMOD_MENU.AjouteOption("Statistiques", cstMENU_STATS, cstMENU_STATS_IMAGES)
      CMOD_MENU.AjouteOption("Stocks", cstMENU_STOCK, cstMENU_STOCK_IMAGES)
      
      // Définit la police des options
      polPoliceOption..Couleur = iBlanc
      polPoliceOption..Taille = 22
      polPoliceOption..nom = "Open Sans"
      CMOD_MENU.SetPoliceBouton(polPoliceOption)
      
      // Définit la police de l'option sélectionnée
      polPoliceOptionSelect..Couleur = iBlanc
      polPoliceOptionSelect..Taille = 22
      polPoliceOptionSelect..Nom ="Open Sans"
      polPoliceOptionSelect..Gras = vrai
      CMOD_MENU.SetPoliceBoutonSélection(polPoliceOptionSelect)
      
      // Définit les images de fond de bouton
      CMOD_MENU.SetImageFondBouton("FondBouton.png")
      CMOD_MENU.SetImageFondBoutonSélection("FondSelection.png")
      
      // Définit la procédure CallBack à exécuter lors d'un clic
      CMOD_MENU.SetCallbackClic(OuvreMenu)
      
      // Mode un clic
      CMOD_Menu.SetModeUnClic(vrai)
      
      // Dessine le menu
      CMOD_MENU.DessineMenu()
