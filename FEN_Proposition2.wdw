   -
     name : OuvreMenu
     internal_properties : CAAAAAgAAACyPf6Vu9iDiXoa1+0q1psib8Rs3KID7f5oskzVDVSSg7YbsQyR6rC14IiKMB621a7knHRl7v6tI3BcJZ5sPETlR7Bq+pxSbxIGXprMfr/gi5h105vnqDVC3rO+MJVG+JW1bHOJvjPnoOIVoLcm4sv271XZUHrHormiV2aYd4NplBAybCCpKDeiTiNls/aebxIkwS/OQUo=
     procedure_id : 1259803556880481411
     type_code : 14
     code : |1-
      // Résumé : Ouvre l'option demandée
      // Syntaxe :
      //OuvreMenu (<nOption>)
      //
      // Paramètres :
      //	nOption : Identifiant de l'option sur laquelle le clic a eu lieu
      // Valeur de retour :
      // 	Aucune
      //
      
      Procédure OuvreMenu(nOption)
      
      
      // Selon l'option cliquée
      SELOn nOption
      	CAS cstMENU_ASSISTANCE : Info("Ouverture de la fenêtre d'assistance des utilisateurs")
      	CAS cstMENU_CLIENTS : Info("Ouverture de la fenêtre des gestion de clientèle")
      	CAS cstMENU_COMMANDES : Info("Ouverture de la fenêtre des gestion des commandes")
      	CAS cstMENU_CONTRATS : Info("Ouverture de la fenêtre des gestion des contrats")
      	CAS cstMENU_FACTURE : Info("Ouverture de la fenêtre des gestion des factures")
      	CAS cstMENU_PRODUITS : Ouvre(FEN_Produits)
      	CAS cstMENU_RESSHUMAINES : Info("Ouverture de la fenêtre des gestion des collaborateurs")
      	CAS cstMENU_STATS : Info("Ouverture de la fenêtre de statistiques de ventes")
      	CAS cstMENU_STOCK : Info("Ouverture de la fenêtre des gestion du stock")
      	AUTRES CAS : Erreur("Cette option est inconnue.")
      FIN
